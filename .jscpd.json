{
  "threshold": 0,
  "reporters": ["html", "console", "badge"],
  "ignore": [
    "**/__tests__/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/coverage/**",
    "**/*.min.js"
  ],
  "absolute": true,
  "gitignore": true,
  "format": ["typescript", "javascript", "tsx", "jsx"],
  "minLines": 5,
  "minTokens": 50,
  "output": "./reports/jscpd"
}
